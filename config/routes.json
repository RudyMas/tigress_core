{
  "routes": [
    {
      "request": "GET",
      "path": "/google/drive/link",
      "controller": "Core\\GoogleDriveController",
      "method": "loadByLink"
    },
    {
      "request": "GET",
      "path": "/help",
      "controller": "tigress\\TigressHelpController",
      "method": "index"
    },
    {
      "request": "POST",
      "path": "/lock-pages/refresh",
      "controller": "Core\\LockPagesController",
      "method": "refreshLock"
    },
    {
      "request": "POST",
      "path": "/lock-pages/release",
      "controller": "Core\\LockPagesController",
      "method": "releaseLock"
    },
    {
      "request": "GET",
      "path": "/phpinfo",
      "controller": "Core\\PhpInfoController",
      "method": "index",
      "level_rights": [
        100
      ]
    },
    {
      "request": "GET",
      "path": "/version",
      "controller": "version\\VersionController",
      "method": "index"
    }
  ]
}